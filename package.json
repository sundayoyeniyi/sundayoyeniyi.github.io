{
  "name": "sundayoyeniyi.github.io",
  "version": "0.0.1",
  "description": "Github profile page",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "clean": "rm -rf build",
    "br-dir": "mkdir -p build/src/scripts",
    "test": "echo browser test in progress ... WIP",
    "build:tsc": "tsc",
    "build:babel": "babel build/src/typescripts --out-dir build/src/amd-scripts",
    "build:browserify": "browserify build/src/amd-scripts/index.js > build/src/scripts/bundle.js",
    "build:test": "mocha build/test/typescripts/**/*test.js",
    "watch:tsc": "watch 'npm run build:tsc' src test spec --wait=5",
    "watch:babel": "watch 'npm run build:babel' build/src/typescripts --wait=2",
    "watch:browserify": "watch 'npm run build:browserify' build/src/amd-scripts --wait=2",
    "watch:test": "watch 'npm run build:test' build/test/typescripts --wait=2",
    "build": "npm-run-all --sequential clean build:tsc build:babel br-dir build:browserify build:test",
    "watch:build": "npm-run-all --sequential build --parallel watch:tsc watch:babel watch:browserify watch:test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sundayoyeniyi/sundayoyeniyi.github.io.git"
  },
  "keywords": [
    "Github",
    "profile",
    "page"
  ],
  "author": "Sunday Oyeniyi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sundayoyeniyi/sundayoyeniyi.github.io/issues"
  },
  "homepage": "https://github.com/sundayoyeniyi/sundayoyeniyi.github.io#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "browser-sync": "^2.11.1",
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-react": "^3.15.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-browserify": "^0.5.1",
    "gulp-changed": "^1.3.0",
    "gulp-compass": "^2.1.0",
    "gulp-cssnano": "^2.1.0",
    "gulp-inject": "^3.0.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.11.0",
    "gulp-uglify": "^1.5.3",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.7",
    "mocha": "^2.4.5",
    "npm-run-all": "^1.5.1"
  }
}
