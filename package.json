{
  "name": "sundayoyeniyi.github.io",
  "version": "0.0.1",
  "description": "Github profile page",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build",
    "tsc": "tsc --outDir build -w",
    "babel": "node_modules/.bin/babel build/src/typescripts --watch --out-dir build/src/scripts",
    "browserify": "node_modules/.bin/browserify build/src/scripts/index.js -o build/src/scripts/bundle.js",
    "watchify": "node_modules/.bin/watchify build/src/scripts/index.js -o build/src/scripts/bundle.js",
    "test": "mocha build/test/typescripts/**/*test.js -w",
    "build": "npm-run-all --parallel tsc babel test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sundayoyeniyi/sundayoyeniyi.github.io.git"
  },
  "keywords": [
    "Github",
    "profile",
    "page"
  ],
  "author": "Sunday Oyeniyi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sundayoyeniyi/sundayoyeniyi.github.io/issues"
  },
  "homepage": "https://github.com/sundayoyeniyi/sundayoyeniyi.github.io#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-react": "^3.15.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-browserify": "^0.5.1",
    "gulp-changed": "^1.3.0",
    "gulp-compass": "^2.1.0",
    "gulp-cssnano": "^2.1.0",
    "gulp-inject": "^3.0.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.11.0",
    "gulp-uglify": "^1.5.3",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.7",
    "mocha": "^2.4.5",
    "npm-run-all": "^1.5.1",
    "watchify": "^3.7.0"
  }
}
